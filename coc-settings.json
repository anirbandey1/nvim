{
  "clangd.enabled": true,
  "clangd.disableDiagnostics": false,
  "clangd.disableCompletion": false,
  "clangd.disableSnippetCompletion": true,
  "snippets.ultisnips.pythonPrompt": false,
  "jedi.enable": true,
  "jedi.startupMessage": false,
  "jedi.diagnostics.enable": true,
  "jedi.diagnostics.didOpen": true,
  "jedi.diagnostics.didChange": true,
  "jedi.diagnostics.didSave": true,
  "jedi.jediSettings.debug": true,
  "python.pythonPath": "~/.venvs/.nvim-venv/bin/python",
  "python.venvPath":["~/.venvs/.nvim-venv"],
    "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    }
  }
}


